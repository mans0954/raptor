                <Component Id="cmpRaptorIcaBinWrapperConf" Guid="*">
                    <File Id="filRaptorIcaBinWrapperConf" KeyPath="yes" Source="source\ica\bin\wrapper.conf" />
                </Component>
            </Directory>
