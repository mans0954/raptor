    </Fragment>
    <Fragment>
        <ComponentGroup Id="cgrRaptorIcaBin">
            <ComponentRef Id="cmpRaptorIcaBinIcaBat" />
            <ComponentRef Id="cmpRaptorIcaBinWrapperExe" />
            <ComponentRef Id="cmpRaptorIcaBinWrapperConf" />
        </ComponentGroup>
    </Fragment>
</Wix>