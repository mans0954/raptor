<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<var name="modelBean" class="com.dummy.trinidad.web.DummyBean" />

	<view-state id="enterData" view="enterData.jspx">

		<transition on="send" to="viewData">

		</transition>
	</view-state>

	<view-state id="viewData" view="viewData.jspx">
		<transition on="back" to="enterData">
			<render fragments="dataFragment" />
		</transition>
	</view-state>

</flow>