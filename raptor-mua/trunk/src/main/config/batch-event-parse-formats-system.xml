<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!--
		definition of the log file format, where some standard log file
		formats will be specified here e.g. shib-access, W3C, apache, EZProxy
		etc IMPORTANT, the headers must be a match for the internal model at
		the moment
	-->

	<!-- accepted types are DATE, STRING, INTEGER -->

    <bean id="openathenslaFormat" class="uk.ac.cardiff.raptor.parse.external.file.format.LogFileFormat">
        <property name="headers">
            <list>
                <bean class="uk.ac.cardiff.raptor.parse.external.file.format.Header">
                    <property name="fieldName">
                        <value>eventTime</value>
                    </property>
                    <property name="fieldNo">
                        <value>0</value>
                    </property>
                    <property name="additionalFieldNos">
                        <list>
                            <value>1</value>
                            <value>2</value>
                        </list>
                    </property>
                    <property name="timeZone">
                        <value>UTC</value>
                    </property>
                    <property name="fieldType">
                        <value>DATE</value>
                    </property>
                    <property name="regexReplaceAll">
                        <map>
                            <entry key="\[" value="" />
                            <entry key="\]" value="" />
                        </map>
                    </property>
                    <property name="dateTimeFormat">
                        <value>dd/MMM/yyyyHH:mm:ss.SSSZ</value>
                    </property>
                </bean>
                <bean class="uk.ac.cardiff.raptor.parse.external.file.format.Header">
                    <property name="fieldName">
                        <value>requesterIp</value>
                    </property>
                    <property name="fieldNo">
                        <value>9</value>
                    </property>
                    <property name="fieldType">
                        <value>STRING</value>
                    </property>
                </bean>
                <bean class="uk.ac.cardiff.raptor.parse.external.file.format.Header">
                    <property name="fieldName">
                        <value>principalName</value>
                    </property>
                    <property name="fieldNo">
                        <value>10</value>
                    </property>
                    <property name="fieldType">
                        <value>STRING</value>
                    </property>
                    <property name="regexReplaceAll">
                        <map>
                            <entry key="&quot;" value="" />
                        </map>
                    </property>
                </bean>
                <bean class="uk.ac.cardiff.raptor.parse.external.file.format.Header">
                    <property name="fieldName">
                        <value>resourceId</value>
                    </property>
                    <property name="fieldNo">
                        <value>11</value>
                    </property>
                    <property name="fieldType">
                        <value>STRING</value>
                    </property>
                </bean>
            </list>
        </property>
        <property name="delimeter">
            <value> </value>
        </property>
    </bean>

