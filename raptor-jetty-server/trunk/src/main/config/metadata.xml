<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="webMetadata" class="uk.ac.cardiff.model.ServiceMetadata">
		<property name="serviceName">
			<value>Cardiff University's RaptorWeb Instance</value>
		</property>
		<property name="organisationName">
			<value>Cardiff University</value>
		</property>
		<property name="contactEmail">
			<value>smartp@cf.ac.uk</value>
		</property>
	</bean>


</beans>
