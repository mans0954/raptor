<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">


	<bean id="webMetadata" class="uk.ac.cardiff.model.ServiceMetadata">
        <property name="organisationName"><value>YOUR_ORGANISATION</value></property>
        <property name="contactEmail"><value>CONTACT_EMAIL</value></property>
        <property name="serviceName"><value>SERVICE_NAME</value></property>       
        <property name="entityId"><value>https://HOSTNAME/raptor-web</value></property>
	</bean>


</beans>
