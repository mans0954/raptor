<!--  this is important, we ignore the eventTime, instead passing the eventTime in a simpler Java Date
property using the getDate and setDate methods -->
<mappings xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<mapping>
		<property name="eventTime" ignore="true"/>
		<property name="persistantId" ignore="true"/>
		<property name="eventTimeMillis" ignore="true"/>
	</mapping>
</mappings>