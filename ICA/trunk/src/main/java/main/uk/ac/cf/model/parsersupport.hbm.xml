<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
         "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
         "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="main.uk.ac.cf.model.ParserSupport" table="parsersupport" dynamic-update="false" dynamic-insert="true" >

	<id name="persistantId" column="persistantId" type="long"
			unsaved-value="null">
			<generator class="native" >
				<param name="sequence">hib_parsersupoprt_seq</param>
    		</generator>
	</id>

 	<property name="lastLineParsed" type="integer" update="true" insert="true" column="lastLineParsed"  not-null="false" />

	</class>


</hibernate-mapping>
