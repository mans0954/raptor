            </wsdl:fault>
        </wsdl:operation>
        <wsdl:operation name="getGeofeatures">
            <soap12:operation soapAction="urn:getGeofeatures" style="document"/>
            <wsdl:input>
                <soap12:body use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap12:body use="literal"/>
            </wsdl:output>
            <wsdl:fault name="ToponymOntologyServiceFault"> <!-- NEW -->
                <wsdl:documentation>
            	  The fault message is defined by the TripodFault
                </wsdl:documentation>
                <soap:fault use="literal"/>
            </wsdl:fault>
        </wsdl:operation>
        <wsdl:operation name="getRoadFeatures">
