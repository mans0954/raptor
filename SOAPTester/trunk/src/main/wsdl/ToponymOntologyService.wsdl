            <wsdl:fault name="ToponymOntologyServiceFault"> <!-- NEW -->
                <wsdl:documentation>
            	  The fault message is defined by the TripodFault
                </wsdl:documentation>
                <soap:fault use="literal"/>
            </wsdl:fault>
        </wsdl:operation>
        <wsdl:operation name="getGeofeaturesFast">
            <soap12:operation soapAction="urn:getGeofeaturesFast" style="document"/>
            <wsdl:input>
                <soap12:body use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap12:body use="literal"/>
            </wsdl:output>
            <wsdl:fault name="ToponymOntologyServiceFault"> <!-- NEW -->
                <wsdl:documentation>
            	  The fault message is defined by the TripodFault
                </wsdl:documentation>
                <soap:fault use="literal"/>
            </wsdl:fault>
        </wsdl:operation>
    </wsdl:binding>


    <wsdl:service name="ToponymOntologyService">
        <wsdl:port name="ToponymOntologyServiceSOAP12port_http" binding="axis2:ToponymOntologyServiceSOAP12Binding">
            <soap12:address location="http://tomcatvm.centrica.dnsalias.net:83/axis2/services/ToponymOntologyService"/>
        </wsdl:port>
    </wsdl:service>




</wsdl:definitions>
