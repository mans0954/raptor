<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="icaRegistry" class="uk.ac.cardiff.RaptorUA.engine.ICARegistry">
		<property name ="ICAEntries">
			<list>
				<bean id="cardiffShibICA" class="uk.ac.cardiff.RaptorUA.model.ICAEntry">
					<property name="serviceEndpoint"><value>http://localhost:8080/ICA/Collector</value></property>
				</bean>
			</list>
		</property>
	</bean>

</beans>
